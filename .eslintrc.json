{
  "extends": ["airbnb", "prettier"],
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": 0
  }
}
